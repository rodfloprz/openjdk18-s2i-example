{
  "kind": "ImageStream",
  "apiVersion": "v1",
  "metadata": {
    "name": "openjdk18-openshift",
    "annotations": {
      "docker.io/image.insecureRepository": "true"
    }
  },
  "spec": {
    "dockerImageRepository": "hub.docker.com/juniorjbn/openjdk18-openshift",
    "tags": [
      {
        "name": "latest",
        "from": {
          "kind": "DockerImage",
          "name": "docker.io/juniorjbn/openjdk18-openshift:latest"
        },
        "importPolicy": {
          "insecure": true
        }
      }
    ]
  }
}
